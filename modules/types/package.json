{
  "name": "@diana-ui/types",
  "version": "0.1.13",
  "main": "lib/types.js",
  "module": "module/types.js",
  "license": "MIT",
  "files": [
    "lib",
    "module"
  ],
  "scripts": {
    "build": "yarn compile:lib && yarn compile:module",
    "compile:lib": "tsc --target es5 --module commonjs --outDir lib",
    "compile:module": "tsc --target es6 --module esnext --outDir module",
    "clean:files": "rimraf lib module",
    "prepare": "yarn clean:files && yarn build",
    "watch": "npm-watch"
  },
  "peerDependencies": {
    "react": "^16.12.0"
  },
  "dependencies": {
    "@diana-ui/tokens": "^0.1.10",
    "aesthetic": "^5.1.0",
    "aesthetic-react": "^2.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "watch": {
    "prepare": {
      "runOnChangeOnly": true,
      "extensions": "ts,tsx",
      "ignore": [
        "lib",
        "node_modules",
        "module"
      ],
      "patterns": [
        "."
      ]
    }
  }
}
