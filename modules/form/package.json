{
  "name": "@diana-ui/form",
  "version": "0.0.1",
  "main": "lib/index.js",
  "module": "module/index.js",
  "files": [
    "lib",
    "module"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn compile:lib && yarn compile:module",
    "compile:lib": "tsc --target es5 --module es2015 --outDir lib",
    "compile:module": "tsc --target es6 --module esnext --outDir module",
    "clean:files": "rimraf lib module",
    "prepare": "yarn clean:files && yarn build"
  },
  "dependencies": {
    "@diana-ui/base": "^0.0.1",
    "@diana-ui/button": "^0.0.1",
    "@diana-ui/checkbox": "^0.0.1",
    "@diana-ui/textinput": "^0.0.1",
    "@diana-ui/types": "^0.0.1",
    "react": "^16.12.0",
    "react-hook-form": "^4.8.2",
    "yup": "^0.28.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
