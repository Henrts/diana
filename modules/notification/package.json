{
  "name": "@diana-ui/notification",
  "version": "0.0.3",
  "main": "lib/index.js",
  "module": "module/index.js",
  "files": [
    "lib",
    "module"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn compile:lib && yarn compile:module",
    "compile:lib": "tsc --target es5 --module es2015 --outDir lib",
    "compile:module": "tsc --target es6 --module esnext --outDir module",
    "clean:files": "rimraf lib module",
    "prepare": "yarn clean:files && yarn build"
  },
  "dependencies": {
    "@diana-ui/base": "^0.0.3",
    "@diana-ui/icon": "^0.0.3",
    "@diana-ui/types": "^0.0.3",
    "@diana-ui/typography": "^0.0.3",
    "aesthetic-react": "^2.1.0",
    "react": "^16.12.0",
    "react-transition-group": "^4.3.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@diana-ui/button": "^0.0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
