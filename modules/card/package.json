{
  "name": "@diana-ui/card",
  "version": "1.0.0",
  "main": "lib/index.js",
  "module": "module/index.js",
  "files": [
    "lib",
    "module"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn compile:lib && yarn compile:module",
    "compile:lib": "tsc --target es5 --module commonjs --outDir lib",
    "compile:module": "tsc --target es6 --module esnext --outDir module",
    "clean:files": "rimraf lib module",
    "prepare": "yarn clean:files && yarn build"
  },
  "dependencies": {
    "@diana-ui/base": "^1.0.0",
    "@diana-ui/button": "^1.0.0",
    "@diana-ui/divider": "^1.0.0",
    "@diana-ui/icon": "^1.0.0",
    "@diana-ui/types": "^1.0.0",
    "@diana-ui/typography": "^1.0.0"
  },
  "devDependencies": {
    "@diana-ui/button": "^0.1.13"
  },
  "peerDependencies": {
    "react": "^16.12.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
