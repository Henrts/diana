{
  "name": "@diana-ui/dropdown",
  "version": "0.0.10",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "module/index.js",
  "files": [
    "lib",
    "module"
  ],
  "scripts": {
    "build": "yarn compile:lib && yarn compile:module",
    "compile:lib": "tsc --target es5 --module es2015 --outDir lib",
    "compile:module": "tsc --target es6 --module esnext --outDir module",
    "clean:files": "rimraf lib module",
    "prepare": "yarn clean:files && yarn build"
  },
  "dependencies": {
    "@diana-ui/base": "^0.0.9",
    "@diana-ui/hooks": "^0.0.9",
    "@diana-ui/popover": "^0.0.9",
    "@diana-ui/types": "^0.0.9",
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@diana-ui/typography": "^0.0.9"
  },
  "publishConfig": {
    "access": "public"
  }
}
