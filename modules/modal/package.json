{
  "name": "@diana-ui/modal",
  "version": "0.0.10",
  "main": "lib/index.js",
  "module": "module/index.js",
  "files": [
    "lib",
    "module"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn compile:lib && yarn compile:module",
    "compile:lib": "tsc --target es5 --module es2015 --outDir lib",
    "compile:module": "tsc --target es6 --module esnext --outDir module",
    "clean:files": "rimraf lib module",
    "prepare": "yarn clean:files && yarn build"
  },
  "peerDependencies": {
    "react": "^16.12.0"
  },
  "dependencies": {
    "@diana-ui/base": "^0.0.10",
    "@diana-ui/icon": "^0.0.10",
    "@diana-ui/types": "^0.0.10",
    "@diana-ui/typography": "^0.0.10",
    "@types/react-modal": "^3.10.4",
    "react-modal": "^3.11.1"
  },
  "devDependencies": {
    "@diana-ui/button": "^0.0.10"
  },
  "publishConfig": {
    "access": "public"
  }
}
