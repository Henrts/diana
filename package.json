{
  "name": "ydesign",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "jsnext:main": "dist/index.es.js",
  "dependencies": {
    "aphrodite": "^2.4.0",
    "babel-preset-react-app": "^9.1.0",
    "eslint-config-prettier": "^6.9.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "aesthetic": "^5.0.1",
